<system.webServer>
    <rewrite>
        <rules>
            <rule name=”Redirect to https”>
            <match url=”(.*)”/>
            <conditions>
                <add input=”{HTTPS}” pattern=”Off”/>
                <add input=”{REQUEST_METHOD}” pattern=”^get$|^post$|^put$|^delete$” />
            </conditions>
            <action type=”Redirect” url=”https://titan-crm.azurewebsites.net”/>
            </rule>
        </rules>
    </rewrite>
</system.webServer>